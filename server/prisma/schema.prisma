generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["linux-musl", "darwin"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model games {
  box_art      String
  description  String?
  developer    String?
  hero         String?
  id           Int     @default(autoincrement()) @id
  release_year Int
  synopsis     String
  title        String
  trailer      String?
}